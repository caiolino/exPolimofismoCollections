package animaisPolimorfismo;

public class Gato extends Animal {
	
	public Gato() {
		super();
	}
	
	public Gato(String nome, int idade) {
		super(nome, idade);
	}
	
	public void fazerSom() {
		this.som = "miado";
		System.out.println(this.som);
	}
	
	public void mover() {
		this.movimento = "s";
		System.out.println(this.movimento);
	}

}
