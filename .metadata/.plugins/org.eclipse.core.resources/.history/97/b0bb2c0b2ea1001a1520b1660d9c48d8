package animaisPolimorfismo;

public class main {

	public static void main(String[] args) {

		Cachorro dog = new Cachorro("maju",3);
		Gato cat = new Gato("Lyra",1);
		Cavalo horse = new Cavalo("cavalo",5);
		
		Veterinario vet = new Veterinario();
		
		vet.examinar(dog);
		vet.examinar(cat);
		vet.examinar(horse);
	}

}
